{
  "name": "icarius-next-mdx-starter",
  "version": "1.3.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest run",
    "analyze": "ANALYZE=true npm run build",
    "prebuild": "tsx scripts/validate-internal-links.ts && tsx scripts/generate-sitemap.ts",
    "typecheck": "tsc --noEmit",
    "codex": "tsx scripts/codex-run.ts",
    "codex:links": "tsx scripts/codex-run.ts \"fix case-study links 404\"",
    "codex:sitemap": "tsx scripts/codex-run.ts \"sitemap.xml generation\"",
    "codex:avatars": "tsx scripts/codex-run.ts \"avatar webp materialize\"",
    "codex:chatbot": "tsx scripts/codex-run.ts \"chatbot popover draggable minimise\"",
    "codex:analytics": "tsx scripts/codex-run.ts \"analytics gtag BookCallClick LeadMagnetSubmit ContactSubmit\""
  },
  "engines": {
    "node": "22.x"
  },
  "dependencies": {
    "@mdx-js/loader": "^2.3.0",
    "@mdx-js/react": "^2.3.0",
    "@next/mdx": "14.2.5",
    "framer-motion": "^11.0.0",
    "lucide-react": "^0.468.0",
    "next": "14.2.5",
    "nodemailer": "^7.0.9",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rehype-autolink-headings": "^7.0.0",
    "rehype-slug": "^6.0.0",
    "remark-gfm": "^4.0.0",
    "resend": "^6.1.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^15.5.4",
    "@playwright/test": "^1.49.1",
    "@tailwindcss/typography": "^0.5.15",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.14.9",
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.7",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.5",
    "jsdom": "^27.0.0",
    "postcss": "^8.4.41",
    "tailwindcss": "^3.4.13",
    "tsx": "^4.16.0",
    "typescript": "^5.6.2",
    "vitest": "^3.2.4"
  },
  "packageManager": "npm@10.8.1"
}
